{
    "name": "lemonde/php-fluent-handler-bundle",
    "description": "Fluent Handler for Monolog",
    "type": "bundle",
    "require": {
        "php": ">=7",
        "fluent/logger": "^1.0",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": { "FluentHandlerBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}
